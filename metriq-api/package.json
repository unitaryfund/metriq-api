{
  "name": "metriq-api",
  "version": "0.1.0",
  "description": "RESTful API for Metriq quantum computer benchmark state-of-the-art web app",
  "main": "index.js",
  "scripts": {
    "test": "jest --runInBand ./coverage",
    "db:schema": "node ./scripts/db/schema.mjs",
    "db:migrate": "node ./scripts/db/migrate.mjs",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed": "npx sequelize-cli db:seed",
    "db:seed:all": "npx sequelize-cli db:seed:all",
    "notify:subscriptions": "node ./scripts/notifySubscriptions.mjs",
    "notify:submissions": "node ./scripts/notifyNewSubmissions.mjs",
    "admin:approve:all": "node ./scripts/approveNewSubmissions.mjs"
  },
  "keywords": [
    "api",
    "restful",
    "json",
    "quantum"
  ],
  "author": "Unitary Fund",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/sequelize": "^4.28.10",
    "bcrypt": "^5.0.1",
    "child-process-promise": "^2.2.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-jwt": "^8.4.1",
    "html-metadata-parser": "2.0.4",
    "jest": "^27.0.4",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^3.1.2",
    "nodemailer": "^6.6.2",
    "nodemon": "^2.0.15",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.5",
    "standard": "^16.0.3",
    "twitter-api-v2": "^1.12.5",
    "uuid": "^8.3.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "sequelize-cli": "^6.4.1"
  }
}
